<odoo>
    <template id="card_list_template" name="Card List">
        <div id="wrap" class="container">
            <h1>Cards</h1>
            <t t-foreach="cards" t-as="card">
                <div class="row">
                    <span t-field="card.cardText" />
                    <span t-field="card.url" />
                    <span t-field="card.source" />
                    <span t-field="card.category" />
                    <span t-field="card.deck" />
                </div>
            </t>
        </div>
    </template>
</odoo>